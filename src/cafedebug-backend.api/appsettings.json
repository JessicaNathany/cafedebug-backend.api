{
  "ConnectionStrings": {
    "CafedebugConnectionStringMySQL": "{connection-string}"
  },
  "JwtSettings": {
    "Issuer": "{issuer}",
    "Audience": "{audience}",
    "SigningKey": "{signing-key}",
    "ValidForMinutes": "{valid-for-minutes}",
    "RefreshTokenValidForMinutes": "{refresh-token-valid-for-minutes}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "Microsoft.AspNetCore.Diagnostics.HealthChecks": "Warning",
        "HealthChecks": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}{NewLine}{Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithEnvironmentName", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "CafeDebug.Backend"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [],
    "Webhooks": [],
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "DetailedErrors": false,
  "Storage": {
    "AWS": {
      "S3": {
        "Bucket": "{bucket-name}",
        "BaseUrl": "{s3-url}",
        "Region": "{region}",
        "ForcePathStyle": "{force-path-style}",
        "UseHttp": "{use-http}",
        "ServiceUrl": "{service-url}"
      }
    }
  }
}